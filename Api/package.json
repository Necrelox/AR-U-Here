    "build": "npm run compile-addon && npm run compile-api && npm run exec-builder",
    "build-start": "npm run build && node prod/app.js",
    "start": "node prod/app.js",
    "test": "jest -c jest.config.json",
    "test:watch": "jest --watch -c jest.config.json",
    "lint": "eslint . --ext .ts",
    "fix-lint": "eslint --fix . --ext .ts"
  },
